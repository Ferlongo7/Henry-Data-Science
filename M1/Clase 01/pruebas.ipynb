    "num = 155\n",
    "lista_restos = []\n",
    "while (num // 2):\n",
    "    lista_restos.append(str(num % 2))\n",
    "    num //= 2\n",
    "if num == 1:\n",
    "    lista_restos.append(str(num))\n",
    "lista_restos.reverse()\n",
    "int(\"\".join(lista_restos))"
